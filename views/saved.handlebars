<div class="container">
	<div class="row">
		<h3>Articles</h3>
		{{#each saved_data}}
			<div class="col-md-12">
				<div class='col-md-6'>
					<h2>{{this.title}}</h2>
					<p>{{this.teaser}}</p>
					<a href="{{this.link}}" target="_blank">more...</a>
					<form class="form-inline" action="/saved/:id" method="post">
						<input type="hidden" name="title" value="{{this.title}}">
						<input type="hidden" name="teaser" value="{{this.teaser}}">
						<input type="hidden" name="link" value="{{this.link}}">

						<button type="submit" class="saved">Done</button>
					</form>
				</div>






			</div>
		{{/each}}
	</div>
</div>